{
  "name": "@vba-blocks/desktop",
  "private": true,
  "version": "0.0.0",
  "description": "vba-blocks Desktop",
  "author": "Tim Hall <tim.hall.engr@gmail.com> (https://github.com/timhall)",
  "main": "src/main.js",
  "scripts": {
    "start": "electron .",
    "build": "next build src/renderer && next export src/renderer",
    "pack": "node scripts/link-modules && electron-builder --dir"
  },
  "build": {
    "appId": "com.vba-blocks.desktop",
    "productName": "vba-blocks",
    "files": ["src/**/*", "!src/renderer", "src/renderer/out"]
  },
  "dependencies": {
    "app-root-path": "^2.0.1",
    "electron-is-dev": "^0.3.0",
    "electron-next": "^3.1.3"
  },
  "devDependencies": {
    "devtron": "^1.4.0",
    "electron": "^1.8.1",
    "electron-builder": "^19.34.2",
    "electron-debug": "^1.4.0",
    "electron-devtools-installer": "^2.2.0",
    "next": "^4.0.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "resolve-pkg": "^1.0.0"
  }
}
