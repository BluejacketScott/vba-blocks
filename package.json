{
  "name": "vba-blocks",
  "version": "0.4.1",
  "repository": "https://github.com/vba-blocks/vba-blocks.git",
  "author": "Tim Hall <tim.hall.engr@gmail.com> (https://github.com/timhall)",
  "license": "MIT",
  "bin": "lib/vba-blocks.js",
  "main": "lib/index.js",
  "scripts": {
    "format": "prettier --write \"**/*.{ts,js}\"",
    "test": "jest --runInBand",
    "typecheck": "tsc",
    "test:e2e": "jest --config e2e.config.js --runInBand",
    "build": "rimraf lib && rollup -c && node scripts/ensure-vendor",
    "build:dev": "cross-env NODE_ENV=development rollup -c",
    "build:addins": "cd addins && node --no-warnings ../scripts/build-addins",
    "build:bootstrap": "cd scripts/bootstrap && node --no-warnings ../../lib/vba-blocks build",
    "version": "yarn build && yarn build:addins && node scripts/ensure-vendor && node scripts/create-packages",
    "postversion": "git push && git push --tags",
    "release": "node scripts/release",
    "clean": "rimraf lib && rimraf dist && rimraf addins/build"
  },
  "prettier": {
    "singleQuote": true,
    "printWidth": 100
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js",
      "json"
    ],
    "testEnvironment": "node",
    "testRegex": "(/__tests__/.*|(\\.|/)(test))\\.(js|ts)$"
  },
  "dependencies": {
    "ansi-colors": "^3",
    "archiver": "^3",
    "conf": "^3",
    "date-fns": "^2",
    "debug": "^4",
    "decompress": "^4",
    "env-paths": "^2",
    "fs-extra": "^7",
    "isomorphic-git": "^0.58",
    "logic-solver": "^2",
    "meant": "^1.0",
    "mri": "^1",
    "node-fetch": "^2",
    "open": "^6",
    "pretty-hrtime": "^1",
    "sanitize-filename": "^1",
    "semver": "^5",
    "tmp": "^0.0",
    "toml-patch": "^0.2",
    "walk-sync": "^1",
    "xml-js": "^1.6"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-syntax-typescript": "^7.2.0",
    "@babel/plugin-transform-modules-commonjs": "^7.2.0",
    "@babel/plugin-transform-typescript": "^7.2.0",
    "@babel/preset-typescript": "^7.1.0",
    "@octokit/rest": "^16.21.1",
    "@types/archiver": "^2",
    "@types/fs-extra": "^5",
    "@types/jest": "^23",
    "@types/node": "^10",
    "@types/node-fetch": "^2.1.7",
    "@types/semver": "^5",
    "@types/tmp": "^0.0",
    "babel-plugin-dynamic-import-node-babel-7": "^2.0.7",
    "babel-plugin-macros": "^2.4.5",
    "builtin-modules": "^3.0.0",
    "cross-env": "^5",
    "dedent": "https://github.com/timhall/dedent.git",
    "dotenv": "^7.0.0",
    "editor": "^1.0.0",
    "jest": "^24",
    "mustache": "^3",
    "npm-run-all": "^4",
    "prettier": "^1",
    "rimraf": "^2",
    "rollup": "^1",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-json": "^3.1.0",
    "rollup-plugin-node-resolve": "^4",
    "rollup-plugin-replace": "^2.1.0",
    "rollup-plugin-terser": "^4.0.3",
    "rollup-plugin-typescript": "^1.0.0",
    "tslib": "^1.9.3",
    "typescript": "^3"
  }
}
